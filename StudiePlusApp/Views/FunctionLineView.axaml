<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
			 d:DesignWidth="393" 
			 d:DesignHeight="70"
             x:Class="StudiePlusApp.Views.FunctionLineView">
	<Grid ColumnDefinitions="*, *, *, *" 
		
		Background="#608cb4">
			<Button CornerRadius="10" Background="White" Grid.Column="0" Height="50" Width="50" Margin="20"/>
			<Button CornerRadius="10" Background="White" Grid.Column="1" Height="50" Width="50" Margin="20"/>
			<Button CornerRadius="10" Background="White" Grid.Column="2" Height="50" Width="50" Margin="20"/>
			<Button CornerRadius="10" Background="White" Grid.Column="3" Height="50" Width="50" Margin="20"/>



	</Grid>

</UserControl>
