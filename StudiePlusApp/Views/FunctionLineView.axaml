<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
			 d:DesignWidth="393" 
			 d:DesignHeight="70"
			 x:CompileBindings="False"
             x:Class="StudiePlusApp.Views.FunctionLineView">
	<Border Width="393" Height="70"	Background="#608cb4" CornerRadius="10" Margin="0,25,0,0">
		<Grid ColumnDefinitions="*,*,*,*,*"
        Background="#608cb4"
        HorizontalAlignment="Center"
        VerticalAlignment="Center">

			<Button Grid.Column="0"
					Content="Skema"
					Command="{Binding GoToScheduleCommand}"
					Background="White"
					CornerRadius="10"
					Margin="5"/>

			<Button Grid.Column="1"
					Content="Fag"
					Command="{Binding GoToCoursesCommand}"
					Background="White"
					CornerRadius="10"
					Margin="5"/>

			<Button Grid.Column="2"
					Content="Karakterer"
					Command="{Binding GoToGradesCommand}"
					Background="White"
					CornerRadius="10"
					Margin="5"/>

			<Button Grid.Column="3"
					Content="Beskeder"
					Command="{Binding GoToMessagesCommand}"
					Background="White"
					CornerRadius="10"
					Margin="5"/>

			<Button Grid.Column="4"
					Content="FravÃ¦r"
					Command="{Binding GoToAbsenceCausesCommand}"
					Background="White"
					CornerRadius="10"
					Margin="5"/>

		</Grid>
	</Border>
	

</UserControl>
